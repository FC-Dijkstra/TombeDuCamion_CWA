{
	"name": "tombe-du-camion",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"API": "node bin/launchapi.js",
		"start": "concurrently \"npm:API\" \"npm:CLIENT\"",
		"CLIENT": "ng serve --configuration development",
		"build": "ng build",
		"watch": "ng build --watch --configuration development",
		"test": "ng test",
		"build:ci": "ng build --base-href /TombeDuCamion_CWA/  --source-map=false --configuration=production",
		"test:ci": "ng test --watch=false --code-coverage --source-map=false  --browsers=ChromeHeadless"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^12.2.15",
		"@angular/cdk": "^12.2.13",
		"@angular/common": "^12.2.15",
		"@angular/compiler": "^12.2.15",
		"@angular/core": "^12.2.15",
		"@angular/forms": "^12.2.15",
		"@angular/platform-browser": "^12.2.15",
		"@angular/platform-browser-dynamic": "^12.2.15",
		"@angular/router": "^12.2.15",
		"@cds/core": "^5.6.1",
		"@clr/angular": "12.0.5",
		"@clr/ui": "12.0.5",
		"@types/jquery": "^3.5.11",
		"ajv": "^8.8.2",
		"concurrently": "^6.5.1",
		"graphql": "^16.2.0",
		"graphql-request": "^3.7.0",
		"rxjs": "~6.6.0",
		"shelljs": "^0.8.4",
		"tslib": "^2.3.0",
		"zone.js": "~0.11.4"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^12.2.14",
		"@angular/cli": "^12.2.14",
		"@angular/compiler-cli": "^12.2.15",
		"@angular/material": "^12.2.13",
		"@types/jasmine": "~3.8.0",
		"@types/node": "^12.20.40",
		"jasmine-core": "~3.8.0",
		"karma": "^6.3.9",
		"karma-chrome-launcher": "~3.1.0",
		"karma-coverage": "~2.0.3",
		"karma-firefox-launcher": "^2.1.2",
		"karma-jasmine": "~4.0.0",
		"karma-jasmine-html-reporter": "~1.7.0",
		"typescript": "~4.3.5"
	}
